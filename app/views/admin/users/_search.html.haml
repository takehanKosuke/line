= search_form_for(@q, url: admin_users_path(params[:id]), method: :get, class:'form-group') do |f|
  .form-group.mt-3
    %h3= f.label "検索"
    .col-sm-6.pl-0= f.search_field :name_cont, placeholder: '名前', class:'form-control'
  .form-group
    .col-sm-6.pl-0= f.search_field :email_cont, placeholder: 'メールアドレス', class:'form-control'
  .form-group
    = f.submit "検索する", class: "btn btn-primary"
